{
  "name": "eslint-config-soybeanjs",
  "version": "0.2.8",
  "description": "SoybeanJS's eslint plugin",
  "author": {
    "name": "SoybeanJS",
    "email": "honghuangdc@gmail.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/eslint-config",
  "repository": {
    "url": "https://github.com/soybeanjs/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/eslint-config/issues"
  },
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./vue": "./lib/configs/vue.js",
    "./vue2": "./lib/configs/vue2.js",
    "./react": "./lib/configs/react.js",
    "./react-native": "./lib/configs/react-native.js",
    "./solid": "./lib/configs/solid.js",
    "./svelte": "./lib/configs/svelte.js",
    "./astro": "./lib/configs/astro.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier . --write '!**/*.{js,jsx,mjs,cjs,json,ts,tsx,mts,cts,vue,svelte,astro}' '!pnpm-lock.yaml'",
    "commit": "soy git-commit",
    "cleanup": "soy cleanup",
    "update-pkg": "soy update-pkg",
    "init-git": "soy init-git-hooks",
    "tsx": "tsx",
    "update-version": "bumpp package.json",
    "publish-pkg": "pnpm -r publish --access public",
    "release": "pnpm update-version && pnpm publish-pkg",
    "prepare": "pnpm -r run stub"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint-config-prettier": "8.7.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-astro": "^0.24.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsonc": "2.6.0",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-svelte": "^2.21.0",
    "eslint-plugin-vue": "9.9.0",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-svelte": "^2.9.0"
  },
  "devDependencies": {
    "@soybeanjs/cli": "0.1.6",
    "@types/eslint": "^8.21.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "astro": "^2.1.2",
    "bumpp": "9.0.0",
    "eslint": "8.35.0",
    "eslint-plugin-solid": "^0.11.0",
    "eslint-plugin-soybeanjs": "link:",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-git-hooks": "^2.8.1",
    "solid-js": "^1.6.13",
    "svelte": "^3.56.0",
    "typescript": "4.9.5",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0",
    "typescript": ">=4.8.0"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soybean git-commit-verify",
    "pre-commit": "pnpm format && pnpm lint"
  }
}
