{
  "name": "eslint-config-soybeanjs",
  "version": "0.5.8",
  "description": "SoybeanJS's eslint config resets",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/eslint-config",
  "repository": {
    "url": "https://github.com/soybeanjs/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/eslint-config/issues"
  },
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./vue": "./lib/configs/vue.js",
    "./vue2": "./lib/configs/vue2.js",
    "./react": "./lib/configs/react.js",
    "./react-native": "./lib/configs/react-native.js",
    "./solid": "./lib/configs/solid.js",
    "./svelte": "./lib/configs/svelte.js",
    "./astro": "./lib/configs/astro.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "commit": "soy git-commit",
    "cleanup": "soy cleanup",
    "update-pkg": "soy ncu",
    "publish-pkg": "pnpm -r publish --access public",
    "release": "soy release && pnpm publish-pkg",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-astro": "0.30.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsonc": "2.11.0",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.1.0",
    "eslint-plugin-solid": "0.13.0",
    "eslint-plugin-svelte": "2.35.1",
    "eslint-plugin-vue": "9.19.2",
    "prettier": "3.1.1",
    "prettier-plugin-astro": "0.12.2",
    "prettier-plugin-svelte": "3.1.2"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.0.0-beta.7",
    "@types/eslint": "8.44.9",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.17",
    "astro": "4.0.4",
    "eslint": "8.55.0",
    "eslint-plugin-soybeanjs": "link:",
    "lint-staged": "15.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "simple-git-hooks": "2.9.0",
    "solid-js": "1.8.7",
    "svelte": "4.2.8",
    "typescript": "5.3.3",
    "vue": "3.3.11"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0",
    "typescript": ">=4.8.0"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
